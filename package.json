{
    "private": true,
    "scripts": {
        "init": "bash scripts/init.sh",
        "dotenv": "bash scripts/dotenv.sh",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "prepare": "husky install",
        "sail": "./vendor/bin/sail up",
        "docker-exec": "docker exec -it",
        "laravel": "dotenv cross-var npm run docker-exec \"%LARAVEL_CONTAINER%\"",
        "php": "dotenv cross-var docker exec %LARAVEL_CONTAINER% -- php",
        "composer": "dotenv cross-var docker exec %LARAVEL_CONTAINER% -- composer",
        "seed": "dotenv cross-var docker exec %LARAVEL_CONTAINER% -- php artisan migrate:fresh --seed",
        "format": "dotenv cross-var docker exec %LARAVEL_CONTAINER% -- ./vendor/bin/ecs --fix && npx prettier-eslint --write \"{resources/js/**/*,*}.{ts,tsx,js,jsx}\"",
